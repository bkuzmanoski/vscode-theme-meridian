{
  "name": "vscode-theme-meridian",
  "version": "0.0.5",
  "displayName": "Meridian Theme",
  "description": "A refined light theme with cohesive colors and complete UI coverage.",
  "publisher": "britown",
  "author": {
    "name": "Brian Kuzmanoski"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Meridian",
